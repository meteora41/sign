
<%= form_with model: @recruiting, url: users_recruiting_path(@recruiting.id) do |f| %>

  <h4>タイトル</h4>
  <%= f.text_field :title %>

  <h4>コメント</h4>
  <%= f.text_area :comment %>

  <h4>希望時間</h4>
  <%= f.text_field :datetime %>

  <h4>集合場所</h4>
  <%= f.text_field :place %>


  <%= f.submit '保存' %>

<% end %>