  <div class="container">
    <div class="mx-auto" style="width: 400px;">
      <h2 class="mt-5 pt-5 mb-5 ml-5 ">管理者　新規登録</h2>

      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <!--%= render "admins/shared/error_messages", resource: resource %-->

        <div class="field mt-4 d-flex justify-content-center">
          <h5>　メールアドレス　　　</h5><%= f.email_field :email, autofocus: true, autocomplete: "email" %>
        </div>

        <div class="field mb-2 mt-4 d-flex justify-content-center">
          <h5>　　パスワード　　　</h5>　<%= f.password_field :password, autocomplete: "new-password" %>
        </div>

        <div class="field mt-4 mb-5 d-flex justify-content-center">
          <h5>パスワード(6文字以上)　</h5><%= f.password_field :password_confirmation, autocomplete: "new-password" %>
        </div>

        <div class="actions ml-5 pl-5">
          <%= f.submit "ログイン" %>
        </div>
      <% end %>
      <!--%= render "admins/shared/links" %-->
    </div>
  </div>